{
  "id": "plugin.top.brzjomo.qwen_mt",
  "plugin_type": "translate",
  "icon": "qwen-color.svg",
  "display": "Qwen MT",
  "homepage": "https://github.com/Brzjomo/pot-app-qwen-mt",
  "needs": [
    {
      "key": "model",
      "display": "模型",
      "type": "select",
      "options": {
        "qwen-mt-flash": "qwen-mt-flash",
        "qwen-mt-turbo": "qwen-mt-turbo",
        "qwen-mt-plus": "qwen-mt-plus"
      }
    },
    
    {
      "key": "apiKey",
      "display": "API Key",
      "type": "input"
    },
    {
      "key": "domains",
      "display": "领域提示 (必须为英语)",
      "type": "input",
      "placeholder": "可选：用于指定翻译领域风格。（必须为英语）"
    },
    {
      "key": "temperature",
      "display": "Temperature [0, 2)",
      "type": "input",
      "placeholder": "可选：默认值 0.65。取值范围：[0, 2)"
    }
  ],
